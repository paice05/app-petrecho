{
  "expo": {
    "name": "Meu Petrecho",
    "slug": "meupetrecholatestapp",
    "version": "1.0.52",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sounds/notification.mp3"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#47141e"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mpaice.meupetrecholatestapp"
    },
    "android": {
      "versionCode": 52,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#47141e"
      },
      "package": "com.mpaice.meupetrecholatestapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "87f50058-d72f-4b85-a5cc-65f1688abdba"
      }
    }
  }
}
